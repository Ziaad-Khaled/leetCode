    "def maxIceCream(costs, coins):\n",
    "    costs = sorted(costs)\n",
    "    output = 0\n",
    "    for cost in costs:\n",
    "        coins -= cost\n",
    "        if coins < 0:\n",
    "            return output\n",
    "        else:\n",
    "            output += 1\n",
    "    return output"
   ]
