    "def isPalindrome(x):\n",
    "    if x < 0:\n",
    "        return False\n",
    "    x_str = str(x)\n",
    "    length = len(x_str)\n",
    "    for i, c in enumerate(x_str):\n",
    "        if c != x_str[length - 1 - i]:\n",
    "            return False\n",
    "\n",
    "    return True"
   ]
