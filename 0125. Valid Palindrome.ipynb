   "source": [
    "def isPalindrome(s: str) -> bool:\n",
    "    s = s.lower()\n",
    "    s = ''.join(char for char in s if char.isalnum())\n",
    "    for i, letter in enumerate(s):\n",
    "        if letter != s[-1-i]:\n",
    "            return False\n",
    "    return True"
   ]
  },
